#MM local-juae-gui-png2c : local-juae-gui-png2c-dirs

JUAEHOSTTOOLSDIR := $(GENDIR)/$(CURDIR)/../../..

local-juae-gui-png2c: $(JUAEHOSTTOOLSDIR)/png2c

